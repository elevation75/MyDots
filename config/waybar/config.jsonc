{
    "layer": "top",
    "position": "top",
    "mod": "dock",
    "exclusive": true,
    "passthrough": false,
    "gtk-layer-shell": true,
    "height": 53,
    "modules-left": ["custom/logo","wlr/workspaces","cpu","memory","disk","custom/sofi","custom/launcher","custom/launcher2","custom/email","custom/chatgpt"],
    "modules-center": ["clock","custom/weather"],
    "modules-right": ["network","custom/wallpaper","battery","pulseaudio","pulseaudio#microphone","tray","custom/light_dark","custom/power_btn"],
    
    "hyprland/window": {
        "format": "{}"
    },

    "custom/chatgpt": {
        "format": "Ó¨ß ChatGPT",
        "on-click": "chat-gpt"
    },
    // Battery
    "battery": {
        "states": {
            // "good": 95,
            "warning": 30,
            "critical": 15
        },
        "format": "{icon}{capacity}%",
        "format-charging": "Ôá¶ {capacity}%",
        "format-plugged": "Ôá¶ {capacity}%",
        "format-alt": "{icon}{time}",
        // "format-good": "", // An empty format will hide the module
        // "format-full": "",
        "format-icons": ["ÔâÑ ", "ÔâÉ", "ÔâÇ ", "ÔâÅ ", "ÔâÄ "]
    },


    "custom/email": {
    	"format": "ÔÉ† Mail",
    	"on-click": "thunderbird"
    }, 
    
    "custom/pacman": {
    "format": "{} Ôëø",
    "interval": 1800,
    "exec": "/home/just/.config/waybar/scripts/update_script.sh",
    "on-click": "exec kitty -e /home/just/.config/waybar/scripts/update_script.sh"
    },

    "custom/logo": {       
        "format": " ",
        "on-click": "pkill wofi; wofi -n",
        "tooltip": false
    },

    "custom/sofi": {
		"format": "Û∞ì¶ Sofirem",
		"on-click": "sofi",
		"tooltip": false
    },
	
    "custom/lock_screen": {
        "format": "ÔÄ£",
        "on-click": "sh -c '(sleep 0.5s; swaylock)' & disown",
        "tooltip": false
    },

    "custom/light_dark": {
        "format": "ÔÑ£",
        "on-click": "~/.config/waybar/scripts/baraction light",
        "tooltip": true,
        "tooltip-format": "{Change waybar theme}"
    },

    "custom/power_btn": {
        "format": "Ôîó",
        "on-click": "sh -c '(sleep 0.5s; wlogout --protocol layer-shell)' & disown",
        "tooltip": false
    },

    "cpu": {
        "interval": 10,
        "format": "Û∞çõ {usage}%",
        "interval": 1,
        "format-alt-click": "click",
        "format-alt": "{icon0}{icon1}{icon2}{icon3} {usage:>2}% Û∞çõ",
        "format-icons": ["‚ñÅ", "‚ñÇ", "‚ñÉ", "‚ñÑ", "‚ñÖ", "‚ñÜ", "‚ñá", "‚ñà"],
        "max-length": 10,
        "on-click": "kitty --start-as=fullscreen --title btop sh -c 'btop'"
    },

    "disk": {
        "interval": 30,
        "format": "ÔáÄ {percentage_used}%",
        "path": "/",
        "tooltip": true,
        "tooltip-format": "HDD - {used} used out of {total} on {path} ({percentage_used}%)",
        "on-click": "kitty --start-as=fullscreen --title btop sh -c 'btop'"
    },

    "memory": {
        "interval": 30,
        "format": "Û∞æÜ {}%",
        "max-length": 10,
        "tooltip": true,
        "tooltip-format": "Memory - {used:0.1f}GB used",
        "on-click": "kitty --start-as=fullscreen --title btop sh -c 'btop'"
    },

    /* "custom/updates": {
        "format": "{}",
        "exec": "~/.config/waybar/scripts/update-sys",
        "on-click": "~/.config/waybar/scripts/update-sys update",
        "interval": 300,
        "tooltip": true
    },
 */
    "wlr/workspaces": {
        "disable-scroll": true,
        "all-outputs": true,
        "on-click": "activate",
        "format": "{icon}",
        "format-icons": {
                "1": "Û∞¨é", //George Russell workspace
                "2": "Û∞¨ì", //Lewis Hamilton one
                "3": "Û∞¨î", //Michael Shumacher
                "4": "Û∞¨õ", //Toto Wolf
                "5": "Û∞é±"
        },
        "sort-by-number": true
    },
    
   "custom/weather": {
        //shows the current weather and forecast
        "tooltip" : true,
        "format" : "{}¬∞C",
        "interval" : 3600,
        "exec" : "/usr/bin/wttrbar --date-format %d-%m-%Y --location Bucharest --hide-conditions",
        /*"exec": "~/.config/waybar/scripts/Weather.sh",*/
        "return-type" : "json"
    },

    "tray": {
        "icon-size": 18,
        "spacing": 20,
    },

    "clock": {
        "format": "{:%H:%M %p}",
        "interval": 60,
        "tooltip": true,
        /*"tooltip-format": "{:ÔÅ≥ %A, %e %B %Y}"*/
        "tooltip-format": "<tt><small>{calendar}</small></tt>",
                "calendar": {
                            "mode"          : "year",
                            "mode-mon-col"  : 3,
                            /*"weeks-pos"     : "right",*/
                            "on-scroll"     : 1,
                            "on-click-right": "mode",
                            "format": {
                                      "months":     "<span color='#ffead3'><b>{}</b></span>",
                                      "days":       "<span color='#ecc6d9'><b>{}</b></span>",
                                      /*"weeks":      "<span color='#99ffdd'><b>W{}</b></span>",*/
                                      /*"weekdays":   "<span color='#ffcc66'><b>{}</b></span>",*/
                                      "today":      "<span color='#ff6699'><b><u>{}</u></b></span>"
                                      }
                            },
                "actions":  {
                            "on-click-right": "mode",
                            "on-click-forward": "tz_up",
                            "on-click-backward": "tz_down",
                            "on-scroll-up": "shift_up",
                            "on-scroll-down": "shift_down"
                            }
    
    },

    "backlight": {
        "device": "intel_backlight",
        "format": "{icon} {percent}%",
        "format-icons": ["Ôóù", "Ôóû", "Ôóü"],
        "on-scroll-up": "brightnessctl set 1%+",
        "on-scroll-down": "brightnessctl set 1%-",
        "min-length": 6
    },

    /* "battery": {
        "states": {
            "good": 95,
            "warning": 30,
            "critical": 20
        },
        "format": "{icon} {capacity}%",
        "format-charging": "Ôá¶ {capacity}%",
        "format-plugged": "Ôá¶ {capacity}%",
        "format-alt": "{time} {icon}",
        "format-icons": ["Ôñç", "Ôïπ", "Ôï∫", "Ôïª", "Ôïº", "ÔïΩ", "Ôïæ", "Ôïø", "ÔñÄ", "ÔñÅ", "Ôï∏"]
    }, */

    "pulseaudio": {
        "format": "{icon} {volume}%",
        "format-muted": "Ôë¶",
		"on-click": "~/.config/waybar/scripts/volume --toggle",
        "on-click-right": "pavucontrol",
        "on-scroll-up": "~/.config/waybar/scripts/volume --inc",
        "on-scroll-down": "~/.config/waybar/scripts/volume --dec",
        "scroll-step": 5,
        "format-icons": {
            "headphone": "ÔÄ•",
            "hands-free": "ÔÄ®",
            "headset": "ÔÄ•",
            "phone": "ÔÇï",
            "portable": "ÔÇï",
            "car": "ÔÜπ",
            "default": ["ÔÄ¶", "ÔÄß", "ÔÄ®"]
        },
		"tooltip": true,
        "tooltip-format": "{icon} at {volume}%"
    },

    "pulseaudio#microphone": {
        "format": "{format_source}",
        "format-source": "Ô°´ {volume}%",
        "format-source-muted": "Ô°¨",
        "on-click": "~/.config/waybar/scripts/volume --toggle-mic",
        "on-click-right": "pavucontrol",
        "on-scroll-up": "~/.config/waybar/scripts/volume --mic-inc",
        "on-scroll-down": "~/.config/waybar/scripts/volume --mic-dec",
        "scroll-step": 5,
		"tooltip": true,
		"tooltip-format": "Ô°´ at {volume}%"
    },

    "custom/waybar-mpris": {
    "return-type": "json",
    "exec": "waybar-mpris --position --autofocus",
    "on-click": "waybar-mpris --send toggle",
    // This option will switch between players on right click.
        "on-click-right": "waybar-mpris --send player-next",
    // The options below will switch the selected player on scroll
        // "on-scroll-up": "waybar-mpris --send player-next",
        // "on-scroll-down": "waybar-mpris --send player-prev",
    // The options below will go to next/previous track on scroll
        // "on-scroll-up": "waybar-mpris --send next",
        // "on-scroll-down": "waybar-mpris --send prev",
    "escape": true,
    },

    "custom/playerctl": {
        "format": "üéù {}",
        "height": 50,
        "return-type": "json",
        "max-length": 40,
        "exec": "playerctl -a metadata --format '{\"text\": \"{{playerName}}: {{artist}} - {{markup_escape(title)}}\", \"tooltip\": \"{{playerName}} : {{markup_escape(title)}}\", \"alt\": \"{{status}}\", \"class\": \"{{status}}\"}' -F",
        "on-click": "playerctl play-pause",
        "on-click-middle":"playerctl previous",
        "on-click-right":"playerctl next"
    },

    "temperature": {
        "thermal-zone": 1,
        "format": "Ôãâ {temperatureC}¬∞C",
        "critical-threshold": 70,
        "format-critical": "Ôãá {temperatureC}¬∞C",
        "on-click": "kitty --start-as=fullscreen --title btop sh -c 'btop'"
    },

    "network": {
        "interface": "enp7s0", // (Optional) To force the use of this interface
        "format-wifi": "  {signalStrength}% Ôá´ ",
        "format-ethernet": "‚ñº{bandwidthDownBytes} ‚ñ≤{bandwidthUpBytes}",   
        "format-linked": "{ifname} (No IP)",
        "format-disconnected": "‚ö† Disconnected ",
        //"tooltip-format": "{essid} {ifname}: {ipaddr}",
		"tooltip-format": "Û∞åó {ipaddr}",
        "interval":"1",
        "on-click": "alacritty -e nmtui"

    },

    "network#vpn": {
        "interface": "tun0",
        "format": "ÔÄí  {essid} ({signalStrength}%)",
        "format-disconnected": "‚ö†  Disconnected",
        "tooltip-format": "{ifname}: {ipaddr}/{cidr}",
    },

    "custom/launcher": {
        "format": "Û∞óÄ Code",
		"return-type": "json",
        "on-click": "~/.config/waybar/scripts/launch_app.sh",
        "interval": 1

    },

    "custom/launcher2": {
        "format": "Û±öç Edit",
        "return-type": "json",
        "on-click": "~/.config/waybar/scripts/launch_app2.sh",
        "interval": 1

    },

    "custom/keybinds": {
        "format":"ÔÑ®",
        "tooltip": true,
        "label": "Help",
        "on-click": "~/.config/hypr/scripts/keybindings.sh",
    },


    "custom/wallpaper": {
        "format": "ÔÄæ ",
        "on-click": "~/.config/hypr/wallpaper-switcher ; /usr/bin/mpv ~/.config/waybar/blup2.mp3",
        "tooltip": true,
        "tooltip-format": "Change wallpaper"
    }
}
