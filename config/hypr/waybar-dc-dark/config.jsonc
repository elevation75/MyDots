    "hyprland/workspaces": {
        "disable-scroll": true,
        "all-outputs": true,
        "on-click": "activate",
        "format": "{icon}",
        "format-icons": {
                "1": "Û∞é§", //George Russell workspace "Û∞¨é"
                "2": "Û∞éß", //Lewis Hamilton one "Û∞¨ì"
                "3": "Û∞é™", //Michael Shumacher "Û∞¨î"
                "4": "Û∞é≠", //Toto Wolf "Û∞¨õ"
                "5": "Û∞é±",
                "6": "Û∞é≥",
                "7": "Û∞é∂",
                "8": "Û∞éπ",
                "9": "Û∞éº",
        },
        "sort-by-number": true
    },
    
   "custom/weather": {
        //shows the current weather and forecast
        "tooltip" : true,
        "format" : "{}¬∞C",
        "interval" : 3600,
        "exec" : "/usr/bin/wttrbar --date-format %d-%m-%Y --location Bucharest --hide-conditions",
        /*"exec": "~/.config/waybar/scripts/Weather.sh",*/
        "return-type" : "json"
    },

    "tray": {
        "icon-size": 18,
        "spacing": 20,
    },

    "clock": {
        "format": "{:%H:%M:%S}",
        "interval": 60,
        "tooltip": true,
        /*"tooltip-format": "{:ÔÅ≥ %A, %e %B %Y}"*/
        "tooltip-format": "<tt><small>{calendar}</small></tt>",
                "calendar": {
                            "mode"          : "year",
                            "mode-mon-col"  : 3,
                            /*"weeks-pos"     : "right",*/
                            "on-scroll"     : 1,
                            "on-click-right": "mode",
                            "format": {
                                      "months":     "<span color='#fff'><b>{}</b></span>",
                                      "days":       "<span color='orange'>{}</span>",
                                      /*"weeks":      "<span color='#99ffdd'><b>W{}</b></span>",*/
                                      /*"weekdays":   "<span color='#ffcc66'><b>{}</b></span>",*/
                                      "today":      "<span color='#ff0000'><b>{}</b></span>"
                                      }
                            },
                "actions":  {
                            "on-click-right": "mode",
                            "on-click-forward": "tz_up",
                            "on-click-backward": "tz_down",
                            "on-scroll-up": "shift_up",
                            "on-scroll-down": "shift_down"
                            }
    
    },
