    "custom/lock_screen": {
        "format": "ÔÄ£",
        "on-click": "sh -c '(sleep 0.5s; swaylock)' & disown",
        "tooltip": false
    },

    "custom/light_dark": {
        "format": "ÔÑ£",
        /*"on-click": "~/.config/waybar/scripts/baraction light",*/
        "on-click" : "$HOME/.config/hypr/change-waybar",
        "tooltip": true,
        "tooltip-format": "Change waybar theme"
    },

    "custom/power_btn": {
        "format": "üèà",
